{
  "name": "puntapie-inicial",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "description": "Puntapié inicial para hacer una aplicación en backend-plus desde cero",
  "main": "dist/server/server-puntapie-inicial.js",
  "files": [
    "dist",
    "install"
  ],
  "dependencies": {
    "@material-ui/core": "~4.9.12",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "^4.0.5",
    "react-redux": "^7.2.0",
    "ajax-best-promise": "^0.3.7",
    "backend-plus": "^1.3.0",
    "best-globals": "~0.10.23",
    "json4all": "~0.3.10",
    "like-ar": "~0.2.19",
    "mini-tools": "~1.0.9",
    "pg-triggers": "~0.2.3",
    "redux-typed-reducer": "~0.1.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.7",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "server4test": "^0.5.3",
    "types.d.ts": "^0.4.17",
    "mixin-patch": "~0.3.3",
    "material-styles": "~0.0.7"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-tests.json || echo \"continue w/error\") && (tsc -p tsconfig-server.json  || echo \"continue w/error\") && (tsc -p tsconfig-unlogged.json || echo \"continue w/error\") && mixin-patch",
    "build-cli": "(tsc -p tsconfig-client.json || echo \"continue w/error\") && (tsc -p tsconfig-unlogged.json || echo \"continue w/error\") && mixin-patch",
    "watch:buildC": "(tsc -p tsconfig-client.json --watch || echo \"continue w/error\")",
    "watch:buildS": "(tsc -p tsconfig-server.json --watch || echo \"continue w/error\")",
    "watch:buildU": "(tsc -p tsconfig-unlogged.json --watch || echo \"continue w/error\")",
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "dump": "npm start -- --dump-db",
    "dumpb": "(tsc -p tsconfig-server.json  || echo \"continue w/error\") && npm run dump",
    "start": "node dist/server/server-puntapie-inicial.js"
  }
}
